public class Solution {

    public int sumOfSumsOfDigits(int input1) {
        String s = String.valueOf(input1);
        int n = s.length();
        int result = 0;

        for (int i = 0; i < n; i++) {
            int currentSum = 0;
            for (int j = i; j < n; j++) {
                currentSum += s.charAt(j) - '0';
            }
            result += currentSum;
        }

        return result;
    }
}
